<h1>UserCookies#index</h1>
<p>Find me in app/views/user_cookies/index.html.erb</p>

<ul>
  <%- @user_cookies.each do |user_cookie| %>
    <li>
      <p>
      Cookie Type: <%= user_cookie.cookie_type.name %>
      </p>
      <p>
      Cookie Type Count: <%= user_cookie.cookie_count %>
      </p>
    </li>
  <% end %>
</ul>
<%= link_to 'Back', user_path(@user) %>
